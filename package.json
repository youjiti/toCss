{
  "name": "tocss",
  "displayName": "toCss",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tocss.toCss",
    "onLanguage:typescriptreact",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tocss.toCss",
        "title": "跳转到less文件"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "tocss.toCss",
          "group": "6_px"
        }
      ]
    },
    "keybindings": [
      {
        "when": "editorHasSelection",
        "command": "tocss.toCss",
        "key": "ctrl+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.11.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/postcss-less": "^4.0.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@babel/parser": "^7.13.9",
    "@babel/traverse": "^7.13.0",
    "postcss": "^8.2.7",
    "postcss-less": "^4.0.1"
  }
}
